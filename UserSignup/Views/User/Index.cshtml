@using UserSignup.Models
@model List<UserSignup.Models.User>


<br />
<h1>Users</h1>

@if (Model.Count == 0)
{
    <p>No users!</p>
    <p>Please <a asp-area="" asp-controller="User" asp-action="Add">Sign up</a>.</p>
}


<br /><br />
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Username</td>
            <td>@user.Email</td>
            <td>@user.CreateDate</td>
            <td>@user.UserId</td>
        </tr>
        
    }
</table>

